    // ##################################################### 보관상품 등록(개별) 메소드(currentTab = 0 ) START ################################
    domeListSend : function(index){
      axios.post('/api/product-manage/v1/dome-list',{
        pageIndex : index ? index : this.domePagination.pageIndex,
        pageSize : this.domePagination.pageSize
      }).then(res => {
        this.domeProductList = res.data.domeggook.list.item;
        this.domePagination.totalCount = res.data.domeggook.header.numberOfItems;
        this.domePagination.pageSize = res.data.domeggook.header.itemsPerPage;
        this.domePagination.pageIndex = res.data.domeggook.header.currentPage;
        EventBus.$emit("domePaginationInfo",this.domePagination)
      }).catch(res => {
        if(res.response.status =="401" || res.response.status =="302"){
          alert("인증 기간이 만료되었습니다.");
          this.$router.go('/');
        }else{
          this.modalMessage=res.response.data;
          this.isModalViewed=true;
        }
      })
    },

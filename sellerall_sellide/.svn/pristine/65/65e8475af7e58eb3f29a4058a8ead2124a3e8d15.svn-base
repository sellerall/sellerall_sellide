    'domeChangeDeli'(value){
      if(value.toLowerCase() == "s"){
        return "무료배송";
      }else if(value.toLowerCase() == "p"){
        return "선결제";
      }else if(value.toLowerCase() == "b"){
        return "착불";
      }else if(value.toLowerCase() == "c"){
        return "선택(착불/선결제)";
      }
    },
    'dbChangeDeli'(value){
      if(value.toLowerCase() == "f"){
        return "무료";
      }else if(value.toLowerCase() == "s"){
        return "선불";
      }else if(value.toLowerCase() == "g"){
        return "착불";
      }else if(value.toLowerCase() == "h"){
        return "선택(착불/선결제)";
      }
    },
    'prodStatus'(value){
      if(value == "1"){
        return "정상";

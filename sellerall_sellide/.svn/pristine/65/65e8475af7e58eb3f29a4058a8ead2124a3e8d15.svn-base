    dbListSend : function(index){
      axios.post('/api/product-manage/v1/db-list',{
        pageIndex : index ? index : this.dbPagination.pageIndex,
        pageSize : this.dbPagination.pageSize
      }).then(res => {
        this.dbProductList = res.data.resultList;
        this.dbPagination.totalCount = res.data.totalCount;
        this.dbPagination.pageSize = res.data.pageSize;
        this.dbPagination.pageIndex = res.data.pageIndex;
        EventBus.$emit("dbPaginationInfo",this.dbPagination)
      }).catch(res => {
        if(res.response.status =="401" || res.response.status =="302"){
          alert("인증 기간이 만료되었습니다.");
          this.$router.go('/');
        }else{
          this.modalMessage=res.response.data;
          this.isModalViewed=true;
        }
      })
    },

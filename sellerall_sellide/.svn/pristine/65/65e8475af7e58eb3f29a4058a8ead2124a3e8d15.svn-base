    menuTabClick: function(item,idx) {
      this.currentTab = idx;
      if(this.currentTab == 0){
        if(this.selectedShop =="Db"){
          EventBus.$emit("dbPaginationInfo",this.dbPagination);
        }else if(this.selectedShop =="Dome"){
          EventBus.$emit("domePaginationInfo",this.domePagination);
        }
      }else if(this.currentTab == 2){
        this.storageListSend();
      }

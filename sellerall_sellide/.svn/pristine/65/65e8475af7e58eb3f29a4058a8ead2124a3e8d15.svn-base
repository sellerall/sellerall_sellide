    domeConvert : function (domeproduct){
      let result = [];
      for(let inData in domeproduct){
        let deliFalg ;
        if(domeproduct[inData].deli.who == "S"){
          deliFalg = "f";
        }else if(domeproduct[inData].deli.who == "P"){
          deliFalg = "s";
        }else if(domeproduct[inData].deli.who == "B"){
          deliFalg = "g";
        }else if(domeproduct[inData].deli.who == "C"){
          deliFalg = "h";
        }
        let adultYn ;
        if(domeproduct[inData].adultOnly == "true"){
          adultYn = "Y";
        }else{
          adultYn = "N";
        }
        let deliPrice ;
        if(domeproduct[inData].deli.fee == "false"){
          deliPrice = 0;
        }else{
          deliPrice = domeproduct[inData].deli.fee;
        }
        let data = {
          "adultYn" : adultYn,
          "partnerCd" : "도매매",
          "deliFlag" : deliFalg,
          "deliPrice" : deliPrice,
          "userId" : domeproduct[inData].id,
          "prodCd" : domeproduct[inData].no,
          "prodPrice" : domeproduct[inData].price,
          "listImg" : domeproduct[inData].thumb,
          "prodNm" : domeproduct[inData].title,
          "minOrderLimit" : domeproduct[inData].unitQty
        }
        result.push(data);
      }
      return result;
    },
